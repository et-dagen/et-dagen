{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "engines": {
    "node": "21.x"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.development",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue,.js,.cjs,.mjs,.yml,\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss,md,yml}": "prettier --write",
    "*.{js,ts,vue}": "eslint"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^8.3.3",
    "@types/node": "^22.0.2",
    "@typescript-eslint/parser": "^8.0.0",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "nuxt": "^3.12.4",
    "nuxt-vitest": "^0.11.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@nuxt/image": "^1.7.0",
    "@pinia/nuxt": "^0.5.2",
    "@tiptap/pm": "^2.5.8",
    "@tiptap/starter-kit": "^2.5.8",
    "@tiptap/vue-3": "^2.5.8",
    "dotenv": "^16.4.5",
    "firebase": "^10.12.4",
    "firebase-admin": "^12.3.0",
    "pinia": "^2.2.0",
    "sass": "^1.77.8",
    "vuetify-nuxt-module": "^0.16.1"
  }
}